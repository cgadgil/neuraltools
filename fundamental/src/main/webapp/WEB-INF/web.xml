<?xml version="1.0" encoding="UTF-8"?>
<web-app version="2.5" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd">
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
    <servlet-name>fundamental</servlet-name>
    <servlet-class>net.gadgil.fundamental.servlets.FundamentalDataServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>regression</servlet-name>
    <servlet-class>net.gadgil.fundamental.servlets.RegressionServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>regression</servlet-name>
    <url-pattern>/regression/*</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>fundamental</servlet-name>
    <url-pattern>/fundie/*</url-pattern>
  </servlet-mapping>
</web-app>
